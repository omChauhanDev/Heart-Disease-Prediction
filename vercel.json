{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/predict/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
